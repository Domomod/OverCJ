#define MY_DEF_1 125 
#include <stdio.h> 
int func1 (int i) 
{     
	int j = 0;     
	for (; j < i; j++)     
	{         
		puts ("Repeat me");     
	}     
	return j; 
} 

void func2 (int start, int range, char *string) 
{     
	int j = start;     
	for (; j < range; j++)     
	{         
		puts (string);     
	}     
	return j; 
} 

void class () 
{     
	puts ("Repeat me"); 
} 

int main () 
{     
	int i;     
	/* This is a for loop */     
	for (i = 0; i < 10; i++)     
	{         
		class ();     
	}     
	func1 (10);     
	func2 (10, 5, MESSAGE); 
	}
