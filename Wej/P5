void class () {     
	puts ("Repeat me"); 
}

int foo (int x, int y, int z) {     
	puts ("Repeat me"); 
}

char* bar (int x) {     
	puts ("Repeat me"); 
}

int main () 
{     
	int i;     
	/* This is a for loop */     
	for (i = 0; i < 10; i++)     
	{         



		puts ("I love Formal Languages and Compilers");
		class ();     
	}     
	func (); 
} 
